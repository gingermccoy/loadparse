def pruneShapes(theList, cutoff):
	global objectsList
	print('pruning shapes small than '+str(cutoff))
	tempList = []
	for shape in theList:
		#print(len(shape))
		if (len(shape)>=cutoff):
			tempList.append(shape)
			#print('added')
	#print(len(tempList))
	objectsList.clear()
	objectsList=tempList.copy()
	tempList.clear()
